{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "hub",
    "editor"
  ],
  "permissions": [
    "core:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-create",
    "fs:allow-remove",
    "fs:allow-exists",
    "fs:allow-stat",
    "fs:allow-fstat",
    "fs:allow-lstat",
    "fs:read-meta",
    "shell:allow-open"
  ]
}
